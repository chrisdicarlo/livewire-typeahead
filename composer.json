{
    "name": "chrisdicarlo/livewire-autocomplete",
    "description": "This is my package LivewireAutocomplete",
    "keywords": [
        "chrisdicarlo",
        "laravel",
        "livewire_autocomplete"
    ],
    "homepage": "https://github.com/chrisdicarlo/livewire-autocomplete",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Di Carlo",
            "email": "chris@dicarlosystems.ca",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/contracts": "^8.0",
        "livewire/livewire": "^2.4",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "ChrisDiCarlo\\LivewireAutocomplete\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ChrisDiCarlo\\LivewireAutocomplete\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ChrisDiCarlo\\LivewireAutocomplete\\LivewireAutocompleteServiceProvider"
            ],
            "aliases": {
                "LivewireAutocomplete": "ChrisDiCarlo\\LivewireAutocomplete\\LivewireAutocompleteFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
